extends ../layout.jade
block content
    .container(style='padding-top: 100px;') 
      div.row  
        div.col-md-7.col-sm-12.col-xs-12
            div#react-root!= react
        div.col-md-5.hidden-xs.hidden-sm
            div#react-user-component!= react-user-component
                
    script(src="/socket.io/socket.io.js")
    script(src='/js/build/bundle.js')   
 block pagescripts 
        script.
            $().ready(function() {
                
                ReactRenderers.renderSlideshowlist('react-root','#{userid}');
                ReactRenderers.renderSlackUser('react-user-component','#{userid}');
                $('#sidebar').affix({
                    offset: {
                    top: 75 
                    }
                });
            });
           
    
            